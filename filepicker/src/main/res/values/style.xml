<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--文字样式-->
    <style name="toolbar_title">
        <item name="android:textSize">21sp</item>
        <item name="android:textColor">@color/filepicker_white</item>
    </style>

    <style name="ThemeBottomDialog">
        <!-- Customize your theme here. -->
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="list_name">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/filepicker_textGrayDark</item>
    </style>

    <style name="text_light_14">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/filepicker_textGrayLight</item>
    </style>

    <style name="item_text_name">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">2</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">#101010</item>
        <item name="android:gravity">left</item>
        <item name="android:textSize">16sp</item>
    </style>

</resources>