<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:height="1px">
        <shape android:shape="rectangle">
            <size android:height="1px" />
            <solid android:color="@color/filepicker_white" />
        </shape>
    </item>

    <item
        android:left="12dp"
        android:right="12dp">
        <shape android:shape="rectangle">
            <size android:height="1px" />
            <solid android:color="@color/filepicker_divider" />
        </shape>
    </item>
</layer-list>